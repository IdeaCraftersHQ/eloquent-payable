{
    "name": "ideacrafters/eloquent-payable",
    "description": "A Laravel package that enables any Eloquent model to accept payments by adding a simple trait",
    "type": "library",
    "keywords": ["laravel", "payments", "stripe", "eloquent", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ideacrafters",
            "email": "hello@ideacrafters.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/events": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/routing": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "stripe/stripe-php": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "laravel/pint": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Ideacrafters\\EloquentPayable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ideacrafters\\EloquentPayable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": [
            "@php -d xdebug.mode=off vendor/bin/pint"
        ],
        "pint:test": [
            "@php -d xdebug.mode=off vendor/bin/pint --test"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ideacrafters\\EloquentPayable\\PayableServiceProvider"
            ],
            "aliases": {
                "Payable": "Ideacrafters\\EloquentPayable\\Facades\\Payable"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
